<%- messages() %>

<h1><%= title %></h1>

<% if (errors) { %>
<ul class="notice">
  <% errors.array().forEach(error => { %>
  <li><%= error.msg %></li>
  <% }) %>
</ul>
<% } %>

<div class="management-container">
  <div class="management-links">
    <h2>Welcome to Account Management</h2>
    <p>You're logged in.</p>
  </div>
</div>
